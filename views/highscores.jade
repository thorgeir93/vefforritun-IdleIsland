extends layout

block content
	p(hidden, id = 'gamestate') #{data.gamestate}
	script(src='/game/playTheme.js')
	div.container
		h1= 'High scores'
		if entries !== false
			hr
			table
				tr
					th Users
					th Scores
				each item in data.entries
					tr
						td #{item.username}
						td #{item.score}
			hr
		p: a(href='/menu') Back to menu